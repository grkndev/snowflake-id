{
  "name": "@grkndev/snowflakeid",
  "version": "1.1.0",
  "description": "A simple Snowflake ID generator for JavaScript and TypeScript",
  "main": "dist/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/esm/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "snowflake",
    "id",
    "generator",
    "typescript",
    "nodejs",
    "javascript",
    "unique",
    "identifier",
    "random",
    "randomId",
    "random-id",
    "random-identifier",
    "random-uuid",
    "random-unique-id",
    "random-unique-identifier",
    "random-unique-uuid",
    "uuid",
    "uniqueid",
    "unique-id",
    "unique-identifier",
    "unique-uuid",
    "unique-snowflake",
    "snowflake-id",
    "snowflake-identifier",
    "snowflake-uuid",
    "snowflake-unique-id",
    "snowflake-unique-identifier",
    "snowflake-unique-uuid"
  ],
  "author": "grkndev",
  "homepage": "https://www.grkn.dev/packages/snowflakeid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grkndev/snowflake-id.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "jest": "^27.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist",
    "src"
  ]
}
